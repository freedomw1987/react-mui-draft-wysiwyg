    "@mui/styles": "^5.5.3",
    "@mui/system": "^5.5.3",
    "@mui/icons-material": "^5.5.1",
    "draft-js": "^0.11.6",
    "prop-types": "latest",
    "react": "^17.0.2",
    "react-is": "^17.0.2"
  },
  "devDependencies": {
    "@emotion/react": "^11.5.0",
    "@emotion/styled": "^11.0.0-next.19",
    "@emotion/cache": "^11.5.0",
